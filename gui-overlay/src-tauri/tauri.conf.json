{
    "$schema": "https://schema.tauri.app/config/2",
    "productName": "A1-Overlay",
    "version": "0.1.0",
    "identifier": "com.a1.overlay",
    "build": {
        "beforeDevCommand": "",
        "devUrl": "http://localhost:1420",
        "beforeBuildCommand": "",
        "frontendDist": "../dist"
    },
    "app": {
        "withGlobalTauri": true,
        "windows": [
            {
                "label": "main",
                "title": "A1 Overlay",
                "width": 200,
                "height": 200,
                "x": 1700,
                "y": 50,
                "resizable": false,
                "fullscreen": false,
                "alwaysOnTop": true,
                "decorations": false,
                "transparent": true,
                "skipTaskbar": true,
                "visible": true,
                "focus": false,
                "center": false,
                "shadow": false
            }
        ],
        "security": {
            "csp": null
        }
    },
    "plugins": {}
}